def distinctSet(sa):
    return len(sa)==len(set(sa))
    
def maximumSubarraySum(nums, k):
    subarr=[]
    max=0
    for i in range(0,len(nums)-k):
        for j in range(0,k):
            subarr.append(nums[i+j])
        if len(subarr)==k and distinctSet(subarr) and sum(subarr)>max:
            max=sum(subarr)
        subarr=[]
    return max
            
nums = [1,5,4,2,9,9,9]
k = 3
print(maximumSubarraySum(nums,k))

