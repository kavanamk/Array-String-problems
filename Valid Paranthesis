def isValid(s):
    stack=[]
    if len(s)%2 !=0:
        return False
    
    for bracket in s:
        if bracket=='(':
            stack.append('(')
        if bracket==')':
            if len(stack)==0:
                pass
            elif not stack.pop()=='(' :
                return False
        
        if bracket=='{':
            stack.append('{')
        if bracket=='}':
            if len(stack)==0:
                pass
            elif not stack.pop()=='{' :
                return False
        
        if bracket=='[':
            stack.append('[')
        if bracket==']':
            if len(stack)==0:
                pass
            elif not stack.pop()=='[' :
                return False
    if len(stack)!=0:
        return False
    return True


print(isValid(s))
