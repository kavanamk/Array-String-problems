s = "Hello World  "
length=0

#remove space from the end
index=len(s)

for letter in s[::-1]:
    
    if letter==' ':
        index-=1
        pass
    else:
        break
    
s=s[:index]

for letter in s[::-1]:
    if letter==' ':
        break
    else:
        length+=1
        
print(length)
    
